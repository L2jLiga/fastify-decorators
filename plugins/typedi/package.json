    "Andrey Chalkin <L2jLiga@gmail.com> (https://github.com/L2jLiga)"
  ],
  "dependencies": {
    "fastify-decorators": "^4.0.0-next.4",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "~16.18.12",
    "fastify": "^4.17.0",
    "jest": "^29.4.1",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": "^14.15.0 || ^16.13.0 || >= 17.0.0"
